<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="momo.com.week10_project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.WelcomeActivity"
            android:theme="@style/WelcomeTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.MainActivity"
            android:theme="@style/MainActivityTheme" />
        <activity
            android:name=".ui.LiveActivity"
            android:theme="@style/LiveActivityTheme" />
        <activity android:name=".ui.SearchActivity" />
        <activity android:name=".ui.LiveDetailActivity" >

        </activity>
        <activity android:name=".ui.Live_lvContent_Activity"
            android:theme="@style/Live_lvContentActivityTheme"></activity>
    </application>

</manifest>